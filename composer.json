{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "aranyasen/laravel-adminer": "^1.0",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "bavix/laravel-wallet": "^5.2",
        "berkayk/onesignal-laravel": "^1.0",
        "cyber-duck/laravel-excel": "^2.2",
        "doctrine/dbal": "^2.10",
        "ejarnutowski/laravel-api-key": "^1.0",
        "fideloper/proxy": "^4.0",
        "genesisgateway/genesis_php": "^1.18",
        "guzzlehttp/guzzle": "^7.3",
        "huddledigital/zendesk-laravel": "^3.5",
        "jurosh/pdf-merge": "^2.0",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravel/vapor-cli": "^1.11",
        "laravel/vapor-core": "^2.10",
        "laravel/vapor-ui": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "milon/barcode": "^7.0",
        "mpociot/laravel-apidoc-generator": "^4.8",
        "picqer/php-barcode-generator": "^2.2",
        "pusher/pusher-php-server": "^4.1",
        "spatie/laravel-medialibrary": "^7.19.0",
        "spatie/laravel-permission": "^3.6",
        "spatie/laravel-webhook-server": "^1.13",
        "symfony/polyfill-mbstring": "^1.23",
        "toin0u/geotools-laravel": "~1.0",
        "tymon/jwt-auth": "^1.0.0",
        "webklex/laravel-pdfmerger": "^1.1"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
